{
	"name": "c19-app",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"deploy": "git checkout master && git merge develop --no-ff --no-edit && git push",
		"update": "node update.js && git add data/last-update.json",
		"no-commit-to-master": "if [ \"$(git rev-parse --abbrev-ref HEAD)\" = 'master' ]; then echo 'You cant commit directly to master branch';exit 1; fi;",
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"export": "next build && next export -o out2",
		"image": "cd automation/ && node make_image.js && cd - && git add automation"
	},
	"bin": {
		"daemon_data": "./daemon.js"
	},
	"pre-commit": [
		"no-commit-to-master",
		"update"
	],
	"dependencies": {
		"@testing-library/jest-dom": "^5.11.4",
		"@testing-library/react": "^11.1.0",
		"@testing-library/user-event": "^12.1.10",
		"bootstrap": "^4.6.0",
		"chart.js": "^2.9.4",
		"color-convert": "^2.0.1",
		"cors": "^2.8.5",
		"csvtojson": "^2.0.10",
		"next": "10.0.5",
		"next-react-svg": "^1.1.2",
		"node-fetch": "^2.6.1",
		"node-schedule": "^2.0.0",
		"react": "17.0.1",
		"react-bootstrap": "^1.4.3",
		"react-chartjs-2": "^2.11.1",
		"react-count-to": "^0.12.0",
		"react-datepicker": "^3.4.1",
		"react-dom": "17.0.1",
		"sass": "^1.32.5",
		"shelljs": "^0.8.4",
		"web-vitals": "^0.2.4",
		"yarn": "^1.22.10"
	},
	"devDependencies": {
		"pre-commit": "^1.2.2"
	}
}
